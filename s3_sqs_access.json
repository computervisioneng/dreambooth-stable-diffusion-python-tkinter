{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::S3-BUCKET",
                "arn:aws:s3:::S3-BUCKET/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "sqs:*",
            "Resource": [
                "SQS-ARN"
            ]
        }
    ]
}
